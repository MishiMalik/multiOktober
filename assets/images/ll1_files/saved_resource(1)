<script>
function adapt_iframe(){
	jQuery('#ifrm-pop .placeholder').fadeOut('fast', function(){
		jQuery('#prev-popup').fadeIn('fast', function(){
			var h = jQuery('#prev-popup').parent().height();
			jQuery('#prev-popup').animate({'height': h+'px'});
		});
	});
}
	function calc_preventivo(link){
		if(link == '' || link == undefined || link == null)
			link = 'https://www.clubmagellano.it/richiedi-preventivo-gratuito/';
		link = link.replace('http:', 'https:');
		if(jQuery('#prev-popup').attr('src') != link){
			jQuery('#prev-popup').hide();
			jQuery('#ifrm-pop .placeholder').show();
			jQuery('#prev-popup').attr('src', link);
			jQuery('#prev-popup').on('load', function(){
				adapt_iframe();
			});
		}
		jQuery('#ifrm-pop').fadeIn('fast');
		jQuery('#ifrm-bg').fadeIn('fast');
	}
	jQuery('#ifrm-bg').on('click', function(){
		jQuery(this).fadeOut('fast');
		jQuery('#ifrm-pop').fadeOut('fast');
	});
</script>
<style>
	#ifrm-pop{
		position:fixed;
		top:60px;
		bottom:60px;
		left:100px;
		right:100px;
		background:#FFF;
		box-shadow:0 0 4px rgba(0, 0, 0, 0.14), 0 4px 8px rgba(0, 0, 0, 0.28);
		z-index:9999;
		padding:20px;
		display:none;
	}
	#ifrm-bg{
		position:fixed;
		top:0;
		bottom:0;
		left:0;
		right:0;
		background:rgba(255,255,255,0.7);
		z-index:9998;
		display:none;
	}
	#ifrm-pop iframe{
		border:none;
		display:none;
	}
	#ifrm-pop .placeholder{ display:block; font-size:100px; text-align:center; margin:40px 0; color:#666;}
	@media screen and (max-width:479px){
		#ifrm-pop{
			top:100px;
			left:0;
			right:0;
			bottom:0;
			padding:0;
		}
	}
</style>
