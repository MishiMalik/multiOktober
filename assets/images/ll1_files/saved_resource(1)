<script src="/js/cookies/src/jquery.cookie.js"></script>
<script type="text/javascript">
	jQuery(document).ready(function(){
		if(jQuery.cookie('CKOKTNET_policy') == 'true'){
			jQuery('#policy-banner').remove();
		}
	});
	jQuery('.policy-close-btn').on('click', function(){
		set_policy_cookie();
	});
	jQuery(document).scroll(function(){
		set_policy_cookie();
	});
	/*jQuery(document).click(function(e){
		if(e.target.parents('#policy-banner').length == 0)
			set_policy_cookie();
	});*/
	function set_policy_cookie(){
		if(jQuery.cookie('CKOKTNET_policy') != 'true'){
			jQuery.cookie('CKOKTNET_policy', 'true', { expires: 365, path: '/' });
			jQuery('#policy-banner').slideUp('fast');
		}
	}
</script>
<div id="policy-banner">
	<div class="policy-content">
		<div class="policy-rationale" style="">
