<script type="text/javascript">
	jQuery(document).ready(function() {
		if (jQuery.cookie('etBloomCookie_undefined') == 'true' || jQuery.cookie('etBloomCookie_undefined') == true) {
			jQuery('.et_bloom_flyin').remove();
		}
	});
	jQuery('.bloom-cookie-close').on('click', function() {
		jQuery.cookie('etBloomCookie_undefined', 'true', {
			expires: 3,
			path: '/'
		});
		var container = jQuery(this).parents('.et_bloom_optin');
		container.addClass('et_bloom_exit_animation');

		setTimeout(function() {
			container.remove();
		}, 400);
		jQuery('body').removeClass('et_bloom_popup_active');
	});

	jQuery('.optin-button').on('click', function() {
		var frasi = new Array('Indica la città di Partenza', 'Inserisci qui la città', 'La tua città qui', 'Inserisci Prima la città', 'Hey! Dimentichi nulla?', 'Dimenticato nulla?', 'QUI la città prima!');
		if (jQuery('#viaggi-okt input[name="citta"]').val() != '') {
			return true;
		} else {
			var rnd = Math.floor((Math.random() * frasi.length) + 1);
			var place = jQuery(this).parents('form').find('input[name="citta"]').attr('placeholder');
			var chose = frasi[rnd];
			while (place == chose) {
				rnd = Math.floor((Math.random() * frasi.length) + 1);
				chose = frasi[rnd];
			}
			jQuery(this).parents('form').effect('bounce');
			jQuery(this).parents('form').find('input[name="citta"]').attr('placeholder', chose);
			return false;
		}
		
	});
</script>
<script>
